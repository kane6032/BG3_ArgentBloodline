local __util = require 'larian.util'

function TransmutedSpellCheck()
    return SpellDamageTypeIs(DamageType.Acid) | SpellDamageTypeIs(DamageType.Cold) | SpellDamageTypeIs(DamageType.Fire) | SpellDamageTypeIs(DamageType.Poison) | SpellDamageTypeIs(DamageType.Lightning) | SpellDamageTypeIs(DamageType.Thunder) | SpellDamageTypeIs(DamageType.Slashing) | SpellDamageTypeIs(DamageType.Piercing) | SpellDamageTypeIs(DamageType.Bludgeoning) | SpellDamageTypeIs(DamageType.Radiant) | SpellDamageTypeIs(DamageType.Necrotic) | SpellDamageTypeIs(DamageType.Force) | SpellDamageTypeIs(DamageType.Psychic)
end